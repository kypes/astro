{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/_astro/*", "/assets/*", "/favicon.ico", "/favicon.svg"],
  "routes": [
    {
      "pattern": "/api/auth/session",
      "function": "api/auth/session"
    },
    {
      "pattern": "/api/auth/openauth/*",
      "function": "api/auth/openauth/_middleware"
    },
    {
      "pattern": "/api/auth/signin/*",
      "function": "api/auth/openauth/_middleware"
    },
    {
      "pattern": "/api/auth/signout",
      "function": "api/auth/openauth/_middleware"
    },
    {
      "pattern": "/api/auth/callback/*",
      "function": "api/auth/openauth/_middleware"
    }
  ]
}
